  "bugs": {
    "url": "https://github.com/reyarqueza/react-webpack-build/issues"
  },
  "homepage": "https://github.com/reyarqueza/react-webpack-build#readme",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/node": "^7.14.2",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "babel-loader": "^8.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
